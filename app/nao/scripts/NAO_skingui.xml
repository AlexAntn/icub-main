<application>
<name>NAO_SKIN</name>
<module>
      <name>iCubSkinGui</name>
      <parameters>--from NAO_lefthand_in.ini --context nao/conf </parameters>
	  <node>icubsrv</node>
	  <stdio>icubsrv</stdio>
	  <tag>LeftHand_in</tag>
 </module>

<connection>
  <from>/icub/skin/lefthand_in</from>
  <to>/skinGui/lefthand_in:i</to>
  <protocol>tcp</protocol>
</connection>
<module>
      <name>iCubSkinGui</name>
      <parameters>--from NAO_lefthand_out.ini --context nao/conf </parameters>
	  <node>icubsrv</node>
	  <stdio>icubsrv</stdio>
	  <tag>LeftHand_out</tag>
 </module>

 <module>
      <name>iCubSkinGui</name>
      <parameters>--from NAO_righthand_in.ini --context nao/conf </parameters>
	  <node>icubsrv</node>
	  <stdio>icubsrv</stdio>
	  <tag>rightHand_in</tag>
 </module>

<connection>
  <from>/icub/skin/righthand_in</from>
  <to>/skinGui/righthand_in:i</to>
  <protocol>tcp</protocol>
</connection>

<module>
      <name>iCubSkinGui</name>
      <parameters>--from NAO_righthand_out.ini --context nao/conf </parameters>
	  <node>icubsrv</node>
	  <stdio>icubsrv</stdio>
	  <tag>rightHand_out</tag>
 </module>

<connection>
