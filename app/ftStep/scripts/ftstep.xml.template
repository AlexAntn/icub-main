        <connection>
                <from>/icub/rightarm/analog:o</from>
                <to>/icub/right_arm/FT:i</to>
                <protocol>udp</protocol>
        </connection>
        <connection>
                <from>/icub/leftleg/analog:o</from>
                <to>/icub/left_leg/FT:i</to>
                <protocol>udp</protocol>
        </connection>
        <connection>
                <from>/icub/rightleg/analog:o</from>
                <to>/icub/right_leg/FT:i</to>
                <protocol>udp</protocol>
        </connection>
