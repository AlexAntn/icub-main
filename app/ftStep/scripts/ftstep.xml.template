<application>
<name>ft Step</name>
	<dependencies>
	        <port>/icub/left_arm/state:o</port>
        </dependencies>
	<module>
      		<name>ftStep</name>
      		<parameters>--from leftArmFT.ini</parameters>
			<node>icub15</node>
			<tag>left_arm_fts</tag>
			<stdio>icub15</stdio>
   	</module>

	<module>
      		<name>ftStep</name>
      		<parameters>--from rightArmFT.ini</parameters>
			<node>icub15</node>
			<tag>right_arm_fts</tag>
			<stdio>icub15</stdio>
   	</module>

	<module>
      		<name>ftStep</name>
      		<parameters>--from leftLegFT.ini</parameters>
			<node>icub15</node>
			<tag>left_leg_fts</tag>
			<stdio>icub15</stdio>
   	</module>

	<module>
      		<name>ftStep</name>
      		<parameters>--from rightLegFT.ini</parameters>
			<node>icub15</node>
			<tag>right_leg_fts</tag>
			<stdio>icub15</stdio>
   	</module>

