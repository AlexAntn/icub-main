<application>
   <name>Grasping Demo Left</name>
   <dependencies>
   </dependencies>

  <module>
    <name>skinDriftCompensation</name>
    <parameters>--from SkinDriftCompensationLeft.ini --context graspingDemo/conf</parameters>
    <node>icub15</node>     
    <stdio>icub15</stdio>
    <tag>drifCompL</tag>
  </module>

  <module>
    <name>iCubSkinGui</name>
    <parameters>--from lefthand.ini --useCalibration</parameters>
    <node>icub15</node>
    <tag>skinGuiL</tag>
  </module>  

  <module>
    <name>tactileGrasp</name>
    <parameters>--from OpenHandLeft.ini --context graspingDemo/conf</parameters>
    <node>icub15</node>
    <stdio>icub15</stdio>
    <tag>openHandL</tag>
  </module>

  <module>
    <name>tactileGrasp</name>
    <parameters>--from CalibrateLeft.ini --context graspingDemo/conf</parameters>
    <node>icub15</node>
    <stdio>icub15</stdio>
    <tag>calibrateL</tag>
  </module>

  <module>
    <name>tactileGrasp</name>
    <parameters>--from ToughGraspLeft.ini --context graspingDemo/conf</parameters>
    <node>icub15</node>
    <stdio>icub15</stdio>
    <tag>toughGraspL</tag>
  </module>

  <module>
    <name>tactileGrasp</name>
    <parameters>--from SoftGraspLeft.ini --context graspingDemo/conf</parameters>
    <node>icub15</node>
    <stdio>icub15</stdio>
    <tag>softGraspL</tag>
  </module>

  <module>
    <name>tactileGrasp</name>
    <parameters>--from CompliantGraspLeft.ini --context graspingDemo/conf</parameters>
    <node>icub15</node>
    <stdio>icub15</stdio>
    <tag>compliantGraspL</tag>
  </module>
   
    
  <!--Connections between SkinDriftCompensation module and the SkinGui-->
  <connection>
    <from>/icub/skin/lefthandcomp</from>
    <to>/skinGui/left_hand:i</to>
    <protocol>tcp</protocol>
  </connection>

</application>

