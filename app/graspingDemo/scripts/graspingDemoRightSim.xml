<application>
   <name>Grasping Demo Right Simulator</name>
   <dependencies>
   </dependencies>

  <module>
    <name>icub_sim</name>
    <parameters></parameters>
    <node>adp_pc</node>
    <tag>simulator</tag>
  </module>

  <module>
    <name>tactileDataSimulation</name>
    <parameters>--context graspingDemo/conf</parameters>
    <node>adp_pc</node>
    <tag>dataSim</tag>
  </module>
  
   <module>
      <name>skinDriftCompensation</name>
      <parameters>--from SkinDriftCompensationRightSim.ini --context graspingDemo/conf</parameters>
	    <node>adp_pc</node>
	    <tag>drifComp</tag>
   </module>

  <module>
    <name>iCubSkinGui</name>
    <parameters>--from righthand.ini --useCalibration</parameters>
	    <node>adp_pc</node>
    <tag>skinGui</tag>
  </module>  

  <module>
    <name>tactileGrasp</name>
    <parameters>--from OpenHandRightSim.ini --context graspingDemo/conf</parameters>
	    <node>adp_pc</node>
    <tag>openHand</tag>
  </module>

  <!--<module>
    <name>tactileGrasp</name>
    <parameters>-from CalibrateRightSim.ini -context graspingDemo/conf</parameters>
	    <node>adp_pc</node>
    <tag>calibrate</tag>
  </module>-->

  <module>
    <name>tactileGraspoo</name>
    <parameters>--from ToughGraspRightSim.ini --context graspingDemo/conf</parameters>
	    <node>adp_pc</node>
    <tag>toughGrasp</tag>
  </module>

  <module>
    <name>tactileGrasp</name>
    <parameters>--from SoftGraspRightSim.ini --context graspingDemo/conf</parameters>
	    <node>adp_pc</node>
    <tag>softGrasp</tag>
  </module>

  <module>
    <name>tactileGrasp</name>
    <parameters>--from CompliantGraspRightSim.ini --context graspingDemo/conf</parameters>
	    <node>adp_pc</node>
    <tag>compliantGrasp</tag>
  </module>
   
    
  <!--Connections between SkinDriftCompensation module and the SkinGui-->
  <connection>
    <from>/icubSim/skin/righthandcomp</from>
    <to>/skinGui/right_hand:i</to>
    <protocol>tcp</protocol>
  </connection>

</application>

