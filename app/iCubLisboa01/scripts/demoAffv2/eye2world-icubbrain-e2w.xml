<application>
  <name>eye2world</name>
  <dependencies>
      <port>/icub/cam/left</port>
      <port>/icub/head/state:o</port>
      <port>/icub/torso/state:o</port>
  </dependencies>
  <module>
      <name>yarpview</name>
      <parameters>--name /e2w_demo/viewer2/img:i --out /e2w_demo/viewer2/click:o</parameters>
      <node>chico3</node>
      <tag>demo_input2</tag>
  </module>
 <module>
      <name>eye2world</name>
      <parameters>--name e2w --context iCubLisboa01/conf --robot icub --eyeCalibration icubEyes.ini --tableConfiguration table.ini</parameters>
      <node>icubbrain2</node>
      <stdio>chico3</stdio>
      <tag>e2w</tag>
  </module>
  <module>
  <name>iKinGazeCtrl</name>
     <node>icubbrain2</node>
      <tag>gazeCtrl</tag>
  </module>
  <connection>
      <from>/icub/cam/left</from>
      <to>/e2w_demo/viewer2/img:i</to>
  </connection>
  <connection>
      <from>/e2w_demo/viewer2/click:o</from>
      <to>/e2w/in</to>
  </connection>
  <connection>
      <from>/icub/head/state:o</from>
      <to>/e2w/icub/head/state:i</to>
  </connection>
  <connection>
      <from>/icub/torso/state:o</from>
      <to>/e2w/icub/torso/state:i</to>
  </connection>
  <connection>
      <from>/e2w/out</from>
      <to>/iKinGazeCtrl/head/xd:i</to>
  </connection>
</application>
