<application>
<name>Joint Reaching Demo (IIT+ISR) Right Hand</name>

        <dependencies>
                <port>/icub/torso/state:o</port>
                <port>/icub/head/state:o</port>
                <port>/icub/right_arm/state:o</port>
                <port>/icub/left_arm/state:o</port>
                <port>/icub/right_leg/state:o</port>
                <port>/icub/left_leg/state:o</port>
        </dependencies>
      
        <module>
                <name>velocityControl</name>
                <parameters>--name icub --part left_arm --period 50</parameters>
                <node>icub12</node>
                <tag>velLeftArm</tag>
        </module>
        
        <module>
                <name>velocityControl</name>
                <parameters>--name icub --part right_arm --period 50</parameters>
                <node>icub12</node>
                <tag>velRightArm</tag>
        </module>
        
        <module>
                <name>velocityControl</name>
                <parameters>--name icub --part left_leg --period 50</parameters>
                <node>icub12</node>
                <tag>velLeftLeg</tag>
        </module>
   
        <module>
                <name>velocityControl</name>
                <parameters>--name icub --part right_leg --period 50</parameters>
                <node>icub12</node>
                <tag>velRightLeg</tag>
        </module>
        
        <module>
                <name>leftEyeToRoot</name>
                <parameters>--inputHeadPort /l2rBall/headIn --inputTorsoPort /l2rBall/torsoIn --inputBallPositionPort /l2rBall/posIn --outputBallPositionPort /l2rBall/posOut</parameters>
                <node>icub12</node>
                <tag>l2rBall</tag>
        </module>

        <module>
                <name>CrawlGeneratorModule</name>
                <parameters>--name icub --part left_arm --file ../config/left_armConfig.ini</parameters>
                <node>icub11</node>
                <tag>genLeftArm</tag>
        </module>
        
        <module>
                <name>CrawlGeneratorModule</name>
                <parameters>--name icub --part right_arm --file ../config/right_armConfig.ini</parameters>
                <node>icub11</node>
                <tag>genRightArm</tag>
        </module>
        
        <module>
                <name>CrawlGeneratorModule</name>
                <parameters>--name icub --part left_leg --file ../config/left_legConfig.ini</parameters>
                <node>icub11</node>
                <tag>genLeftLeg</tag>
        </module>
        
        <module>
                <name>CrawlGeneratorModule</name>
                <parameters>--name icub --part right_leg --file ../config/right_legConfig.ini</parameters>
                <node>icub11</node>
                <tag>genRightLeg</tag>
        </module>

        <module>
                <name>CrawlManagerModule</name>
                <parameters>--name icub --file ../config/managerConfig.ini</parameters>
                <node>icub3</node>
                <tag>manager</tag>
        </module>
  
        <module>
                <name>FeedbackModule</name>
                <parameters>--name icub --period 50</parameters>
                <node>icub3</node>
                <tag>feedback</tag>
        </module>

        <connection>
                <from>/left_arm/vcControl</from>
                <to>/icub/vc/left_arm/input</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/left_arm/vcFastCommand</from>
                <to>/icub/vc/left_arm/fastCommand</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/right_arm/vcControl</from>
                <to>/icub/vc/right_arm/input</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/right_arm/vcFastCommand</from>
                <to>/icub/vc/right_arm/fastCommand</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/right_leg/vcControl</from>
                <to>/icub/vc/right_leg/input</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/right_leg/vcFastCommand</from>
                <to>/icub/vc/right_leg/fastCommand</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/left_leg/vcControl</from>
                <to>/icub/vc/left_leg/input</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/left_leg/vcFastCommand</from>
                <to>/icub/vc/left_leg/fastCommand</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/torso/vcControl</from>
                <to>/icub/vc/torso/input</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/torso/vcFastCommand</from>
                <to>/icub/vc/torso/fastCommand</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/head/vcControl</from>
                <to>/icub/vc/head/input</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/head/vcFastCommand</from>
                <to>/icub/vc/head/fastCommand</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/left_arm/cpg_status/out</from>
                <to>/right_arm/cpg_status/in</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/left_arm/cpg_status/out</from>
                <to>/right_leg/cpg_status/in</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/left_arm/cpg_status/out</from>
                <to>/left_leg/cpg_status/in</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/right_arm/cpg_status/out</from>
                <to>/left_arm/cpg_status/in</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/right_arm/cpg_status/out</from>
                <to>/right_leg/cpg_status/in</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/right_arm/cpg_status/out</from>
                <to>/left_leg/cpg_status/in</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/left_leg/cpg_status/out</from>
                <to>/left_arm/cpg_status/in</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/left_leg/cpg_status/out</from>
                <to>/right_leg/cpg_status/in</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/left_leg/cpg_status/out</from>
                <to>/right_arm/cpg_status/in</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/right_leg/cpg_status/out</from>
                <to>/left_arm/cpg_status/in</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/right_leg/cpg_status/out</from>
                <to>/left_leg/cpg_status/in</to>
                <protocol>tcp</protocol>
        </connection>
        
        <connection>
                <from>/right_leg/cpg_status/out</from>
                <to>/right_arm/cpg_status/in</to>
                <protocol>tcp</protocol>
        </connection>    
        
        <connection>
                <from>/manager/left_arm/out</from>
                <to>/left_arm/parameters/in</to>
                <protocol>tcp</protocol>
        </connection>  
        
        <connection>
                <from>/manager/right_arm/out</from>
                <to>/right_arm/parameters/in</to>
                <protocol>tcp</protocol>
        </connection>    
        
        <connection>
                <from>/manager/left_leg/out</from>
                <to>/left_leg/parameters/in</to>
                <protocol>tcp</protocol>
        </connection> 
        
        <connection>
                <from>/manager/right_leg/out</from>
                <to>/right_leg/parameters/in</to>
                <protocol>tcp</protocol>
        </connection> 

</application>
