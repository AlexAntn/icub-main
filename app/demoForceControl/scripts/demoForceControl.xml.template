<application>
<name>Zero Force Control</name>
	<dependencies>
	        <port>/icub/left_arm/state:o</port>
        </dependencies>
	<module>
      		<name>zeroForceControl</name>
      		<parameters>--context zeroForceControl/conf --from leftArmFT.ini</parameters>
			<node>pc104</node>
			<tag>left_arm_imp</tag>
			<stdio>icub14</stdio>
   	</module>

	<module>
      		<name>zeroForceControl</name>
      		<parameters>--context zeroForceControl/conf --from rightArmFT.ini</parameters>
			<node>pc104</node>
			<tag>right_arm_imp</tag>
			<stdio>icub14</stdio>
   	</module>

	<module>
      		<name>zeroForceControl</name>
      		<parameters>--context zeroForceControl/conf --from leftLegFT.ini</parameters>
			<node>pc104</node>
			<tag>left_leg_imp</tag>
			<stdio>icub14</stdio>
   	</module>

	<module>
      		<name>zeroForceControl</name>
      		<parameters>--context zeroForceControl/conf --from rightLegFT.ini</parameters>
			<node>pc104</node>
			<tag>right_leg_imp</tag>
			<stdio>icub14</stdio>
   	</module>

        <connection>
                <from>/icub/left_arm/analog:o</from>
                <to>/imp/left_arm/FT:i</to>
                <protocol>udp</protocol>
        </connection>
        <connection>
                <from>/icub/right_arm/analog:o</from>
                <to>/imp/right_arm/FT:i</to>
                <protocol>udp</protocol>
        </connection>
        <connection>
                <from>/icub/left_leg/analog:o</from>
                <to>/imp/left_leg/FT:i</to>
                <protocol>udp</protocol>
        </connection>
        <connection>
                <from>/icub/right_leg/analog:o</from>
                <to>/imp/right_leg/FT:i</to>
                <protocol>udp</protocol>
        </connection>
</application>
