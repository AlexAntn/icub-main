            index="1"
            color="Yellow"
            type="lines"
            size="1" />
     <graph remote="/controlBoardDumper/torso/getTorque"
            index="1"
            color="Green"
            type="lines"
            size="1" />
     <graph remote="/controlBoardDumper/torso/getTrqPidReferences"
            index="1"
            color="Blue"
            type="lines"
            size="1" />
    </plot>
    <plot gridx="0"
          gridy="1"
          title="torso_pitch"
          size="60"
          minval="-24"
          maxval="24"
          bgcolor="LightSlateGrey">
     <graph remote="/controlBoardDumper/torso/getTorqueErrors"
            index="2"
            color="Yellow"
            type="lines"
            size="1" />
     <graph remote="/controlBoardDumper/torso/getTorque"
            index="2"
            color="Green"
            type="lines"
            size="1" />
     <graph remote="/controlBoardDumper/torso/getTrqPidReferences"
            index="2"
            color="Blue"
            type="lines"
            size="1" />
    </plot>
  
</portscope>
