<?xml version="1.0" encoding="UTF-8" ?>
<portscope rows="1" columns="3" carrier="udp">
    <plot gridx="0"
          gridy="0"
          title="torso_yaw"
          size="60"
          minval="-24"
          maxval="24"
          bgcolor="LightSlateGrey">
     <graph remote="/controlBoardDumper/torso/getTorqueErrors"
            index="0"
            color="Yellow"
            type="lines"
            size="1" />
     <graph remote="/controlBoardDumper/torso/getTorque"
            index="0"
            color="Green"
            type="lines"
            size="1" />
     <graph remote="/controlBoardDumper/torso/getTrqPidReferences"
            index="0"
            color="Blue"
            type="lines"
            size="1" />
    </plot>
    <plot gridx="0"
          gridy="1"
          title="torso_roll"
          size="60"
          minval="-24"
          maxval="24"
          bgcolor="LightSlateGrey">
