<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE devices PUBLIC "-//YARP//DTD robotInterface 1.0//EN" "http://www.icub.org/DTD/robotInterfaceV1.0.dtd">

<devices robot="coman" build="1">
    
    <!-- left_arm -->
    <device name="vo_left_arm_wrapper" type="analogServer">
      <param name="period">     20                </param>
      <param name="deviceId">   left_arm       </param>
      <param name="total_taxels"> 8               </param>

      <paramlist name="ports">
           <elem name="speeds">    0 7  0 7  </elem>
      </paramlist>

      <action phase="startup" level="5" type="attach">
        <paramlist name="networks">
            <!-- The param value must match the device name in the corresponding emsX file -->
          <elem name="FirstSetOfJoints"> vo_left_arm </elem>
        </paramlist>
      </action>

      <action phase="shutdown" level="5" type="detach" />
   </device>

</devices>


