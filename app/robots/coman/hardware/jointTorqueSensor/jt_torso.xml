<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE devices PUBLIC "-//YARP//DTD robotInterface 1.0//EN" "http://www.icub.org/DTD/robotInterfaceV1.0.dtd">

<devices robot="coman" build="1">
    <device name="jt_torso" type="comanJointTorqueSensor">
        <params file="general.xml" />
            <group name="GENERAL">
                <param name="joints">   3 </param>
                <param name="bIdMap">           1       2       3       </param>
                <param name="axisMap">          0       1       2       </param> 
                <!-- valore di conversione da mRad a gradi Ã¨ 1745.33  (punto, non virgola)-->
                <param name="encoder">          1745.33 1745.33 1745.33 </param>
                <param name="zeros">            0       0       0       </param>
                <param name="maxTorque">        50      50      50      </param>
                <param name="newtonsToSensor">  1000    1000    1000    </param>
            </group>
    </device>
</devices>
