<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE devices PUBLIC "-//YARP//DTD robotInterface 1.0//EN" "http://www.icub.org/DTD/robotInterfaceV1.0.dtd">

<devices robot="coman" build="1">
  <device name="right_leg_ft_dev" type="comanFTsensor">

    <params file="robolli.xml" />     <!-- robolli config -->
    
    <group name="ANALOG">
        <param name="numberOfSensors">    1                       </param>
        <!--  number of sensor channels for each sensor -->
        <param name="channels">           6                       </param> 
        <!--  boards Id  -->
        <param name="FTmap">              34             </param>
        <param name="UseCalibration">     0               </param>
        <param name="Calibration">        0               </param>    
        <param name="newtonsToSensor">    1000            </param>  
    </group>   

  </device>
</devices>
