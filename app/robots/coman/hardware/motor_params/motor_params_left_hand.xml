<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE params PUBLIC "-//YARP//DTD robotInterface 1.0//EN" "http://www.icub.org/DTD/robotInterfaceV1.0.dtd">

<params robot="coman" build="1">

      <!--   conversion from mRad to degree is 1745.33  (use dot, not comma) -->
 
    <group name="GENERAL">
        <param name="joints">   1      </param>
        <param name="bIdMap">           33          </param>
        <param name="axisMap">          0           </param> 
        <param name="encoder">          1745.33     </param>
        <param name="zeros">            0           </param>
        <param name="maxTorque">        5          </param>
        <param name="newtonsToSensor">  1000        </param>  
    </group>
</params>
