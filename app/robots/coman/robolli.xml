<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE params PUBLIC "-//YARP//DTD robotInterface 1.0//EN" "http://www.icub.org/DTD/robotInterfaceV1.0.dtd">

<params robot="coman" build="1">
    <group name="GENERAL">
        <param name="YAML">  "/home/icub/.local/share/yarp/robots/coman/basic.yaml"  </param>
    </group>
    
    <group name="MC_BOARD">
        <!-- old version  -->
        <param name="policy">           0x28FF  </param>
        <param name="extra_policy">     0x0004  </param> 
      <!-- new version 
        <param name="policy">           0x89BB  </param>
        <param name="extra_policy">     0x10C1  </param>    -->
        <param name="bc_rate">          2       </param>
    </group>
      
    <group name="FT_BOARD">
        <param name="policy">           0x2     </param>
        <param name="bc_rate">          2       </param>
        <param name="calibrate_offset"> true    </param>
    </group>
</params>
