<?xml version="1.0" encoding="UTF-8" ?>
<devices>
  <device name="head_face_mc_wrapper" type="controlboardwrapper2">
      <param name="threadrate">  10  </param>
      <paramlist name="networks">
      <elem name="head_face_joints">  0 11 0 11 </elem>
      </paramlist>

      <param name="period"> 10          </param>
      <param name="name">   icub/head   </param>
      <param name="ports">  head        </param>
      <param name="joints"> 12           </param>


      <action phase="startup" level="5" type="attach">
      <paramlist name="networks">
         <elem name="head_face_joints">  head_face_mc </elem>
      </paramlist>
      </action>

      <action phase="shutdown" level="5" type="detach" />
  </device>
</devices>