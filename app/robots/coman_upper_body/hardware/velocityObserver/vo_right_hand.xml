<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE devices PUBLIC "-//YARP//DTD robotInterface 1.0//EN" "http://www.icub.org/DTD/robotInterfaceV1.0.dtd">

<devices robot="coman" build="1">
    <device name="vo_right_hand" type="comanVelocityObserver">
        <params file="general.xml" />
            <group name="GENERAL">
                <param name="joints">  1 </param>
                <param name="bIdMap">           32    </param>
                <param name="axisMap">          0   </param>
                <!-- valore di conversione da mRad a gradi Ã¨ 1745.33  (punto, non virgola)-->
                <param name="encoder">          1745.33  </param>
                <param name="zeros">            0  </param>
                <param name="maxTorque">        5  </param>
                <param name="newtonsToSensor"> 1000 </param>
            </group>
    </device>
</devices>
