<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE robot PUBLIC "-//YARP//DTD robotInterface 1.0//EN" "http://www.icub.org/DTD/robotInterfaceV1.0.dtd">

<robot name="iCubGenova04" build="1" portprefix="icub">
    <params file="hardware/electronics/pc104.xml" />


    <!-- cartesiani --> 
    <devices file="cartesian/left_arm_cartesian.xml" />
    <devices file="cartesian/right_arm_cartesian.xml" />
    

    <!-- HEAD -->	
    <devices file="hardware/cfw002.xml" />
    <devices file="wrappers/head-mcWrapper.xml" />

    <!-- TORSO -->
    <devices file="hardware/ems5.xml" />
    <devices file="wrappers/torso-mcWrapper.xml" />


    <!-- LEFT ARM -->
    <devices file="hardware/ems1.xml" />
    <devices file="hardware/ems2.xml" />
    <devices file="wrappers/left_arm-mcWrapper.xml" /> 


    <!-- RIGHT ARM -->
    <devices file="hardware/ems3.xml" />
    <devices file="hardware/ems4.xml" />
    <devices file="wrappers/right_arm-mcWrapper.xml" />



    

     <!-- ANALOG SENSOR WRAPPERs TORQUE -->
