     <devices file="wrappers/right_arm-asWrapper.xml" />


    <!-- VIRTUAL ANALOG SERVERs -->

    <devices file="wrappers/left_arm-VFTserver.xml" />    
    <devices file="wrappers/right_arm-VFTserver.xml" /> 
    <devices file="wrappers/torso-VFTserver.xml" /> 

    <!-- ANALOG SENSOR WRAPPERs MAIS -->
    <devices file="wrappers/left_arm-asWrapperMais.xml" /> 
    <devices file="wrappers/right_arm-asWrapperMais.xml" /> 

    
 


    
    <!-- INERTIAL SENSOR -->
    <devices file="hardware/inertial.xml" />

    <!--  SKINS -->
    <devices file="wrappers/left_arm-skinWrapper.xml" />
    <devices file="wrappers/right_arm-skinWrapper.xml" />  
    <devices file="wrappers/torso-skinWrapper.xml" />    <!-- e' su can -->
    

    <!--  CALIBRATORS -->
    <devices file="calibrators/head-calib.xml" /> 
    <devices file="calibrators/torso-calib.xml" /> 
    <devices file="calibrators/left_arm-calib.xml" /> 
    <devices file="calibrators/right_arm-calib.xml" />


</robot> 


