<application>
  <name>BIControlGaze starter</name>
  <dependencies>
    <port>/icub01</port>
    <port>/rea/BMLEngine/outCommand</port>
    <port>/rea</port>
  </dependencies>
  <module>
    <name>yarpview</name>
    <parameters>--name /rea/view0 --x 0 --y 10 --synch</parameters>
    <node>rea</node>
    <tag>view0</tag>
  </module>
  <module>
    <name>BIControlGazeInterface</name>
    <parameters></parameters>
    <node>rea</node>
    <stdio>rea</stdio>
    <tag>interface</tag>
  </module>
  <module>
    <name>BIControlGazeEngine</name>
    <parameters></parameters>
    <node>rea</node>
    <stdio>rea</stdio>
    <tag>engine</tag>
  </module>
  <connection>
    <output>/rea/BIControlGazeInterface/out</output>
    <input>/rea/BIControlGazeEngine/inCmd</input>
    <protocol>tcp</protocol>
  </connection>
</application>

