<application>
  <name>Watershed Module starter</name>
  <dependencies>
    <port>/rea/ImageProcessor/out</port>
    <port>/rea</port>
  </dependencies>
  <module>
    <name>yarpview</name>
    <parameters>--name /rea/outBlobsView --x 0 --y 10 --synch</parameters>
    <node>/rea</node>
    <tag>outBlobsView</tag>
  </module>
  <module>
    <name>WatershedModule</name>
    <parameters></parameters>
    <node>/rea</node>
	 <as>watershedmodule</as>
    <tag>watershed</tag>
  </module>
  <connection>
    <output>/rea/ImageProcessor/out</output>
    <input>/rea/Watershed/in</input>
    <protocol>tcp</protocol>
  </connection>
  <connection>
    <output>/rea/ImageProcessor/outRed</output>
    <input>/rea/Watershed/inRed</input>
    <protocol>tcp</protocol>
  </connection>
  <connection>
    <output>/rea/ImageProcessor/outBlue</output>
    <input>/rea/Watershed/inBlue</input>
    <protocol>tcp</protocol>
  </connection>
  <connection>
    <output>/rea/ImageProcessor/outGreen</output>
    <input>/rea/Watershed/inGreen</input>
    <protocol>tcp</protocol>
  </connection>
  <connection>
    <output>/rea/ImageProcessor/outRG</output>
    <input>/rea/Watershed/inRG</input>
    <protocol>tcp</protocol>
  </connection>
  <connection>
    <output>/rea/ImageProcessor/outGR</output>
    <input>/rea/Watershed/inGR</input>
    <protocol>tcp</protocol>
  </connection>
  <connection>
    <output>/rea/ImageProcessor/outBY</output>
    <input>/rea/Watershed/inBY</input>
    <protocol>tcp</protocol>
  </connection>
  <connection>
    <output>/rea/Watershed/outBlobs</output>
    <input>/rea/outBlobsView</input>
    <protocol>tcp</protocol>
  </connection>
</application>

